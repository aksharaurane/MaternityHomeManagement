
<div><center><h1>Nitave Hospital</h1></center></div>
<center><div><h4>Doctor Registration</h4></center></div><br>
<%= form_for :article do |f| %>
<div class="container">
<div class="row">
<div class="col-md-6">
<%= f.label :Name %>
    <%= f.text_field :Name %>
</div>
<div class="col-md-6">
<%= f.label :Doctor_ID  %>
    <%= f.text_field :Doctor_ID %>
</div>
</div>

<div class="row">
<div class="col-md-6">
<%= f.label :Age %>
    <%= f.text_field:Age %>
</div>
<div class="col-md-6">
<%= f.label :Gender %>
    <%= f.text_field:Gender %>
</div>
</div>
<div class="row">
<div class="col-md-6">
<%= f.label :Birth_date %>
<%= f.text_field:Birth_date %>
</div>
<div class="col-md-6">
<%= f.label :Address  %>
    <%= f.text_area:Address %>
</div>
</div>
<div class="row">
<div class="col-md-6">
<%= f.label :Occupation %>
    <%= f.text_field:Occupation %>
</div>
</div>
<div class="row">
<div class="col-md-6">
<%= f.label :User_Name  %>
    <%= f.text_field:User_Name %>
</div>

</div>
<div class="row">
<div class="col-md-6">
<%= f.label :Password  %>
    <%= f.text_field:Password %>
</div>

</div>

<div class="row">
<div class="col-md-6">
<%= f.label :Email_ID  %>
    <%= f.text_field:Email_ID %>
</div>

</div>
<div class="row">
<div class="col-md-6">
<%= f.label :Phone_No  %>
    <%= f.text_field:Phone_No %>
</div>

</div>

    <center><%= f.submit %></center>
  </div>
<% end %>
  
 
</div>
